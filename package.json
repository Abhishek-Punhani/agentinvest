{
  "name": "AgentInvest",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "test": "turbo run test",
    "test:cov": "turbo run test:cov",
    "celery": "turbo run --concurrency 13 celery:trading celery:nse celery:news celery:lowrisk celery:allocation celery:market celery:general celery:streaming celery:regime celery:beat pathway:orders",
    "celery:beat": "turbo run celery:beat",
    "studio": "turbo run studio studio:kafka",
    "studio:all": "concurrently \"turbo run studio\" \"streamlit run kafkaStudio.py\"",
    "docker-logs": "turbo run --concurrency 19 logs:databases logs:redis logs:auth logs:notif logs:portfolio logs:celery-trading logs:celery-nse logs:celery-news logs:celery-lowrisk logs:celery-allocation logs:celery-market logs:celery-general logs:celery-streaming logs:celery-beat logs:kafka logs:frontend logs:alphacopilot logs:alphacopilot-celery"
  },
  "dependencies": {
    "@prisma/client": "5.22.0",
    "@sendgrid/mail": "^8.1.6",
    "axios": "^1.13.2",
    "body-parser": "^2.2.1",
    "bullmq": "^5.63.0",
    "compression": "^1.8.1",
    "connect-redis": "^9.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-fileupload": "^1.0.2",
    "express-rate-limit": "^8.2.1",
    "express-session": "^1.18.2",
    "helmet": "^8.1.0",
    "http-errors": "^2.0.0",
    "ioredis": "^5.8.2",
    "memorystore": "^1.0.2",
    "morgan": "^1.10.1",
    "redis": "^5.0.0",
    "socket.io": "^4.8.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.6",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/http-errors": "^2.0.5",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.10.0",
    "concurrently": "^9.2.1",
    "prettier": "^3.6.2",
    "prisma": "5.22.0",
    "turbo": "^2.6.0",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
