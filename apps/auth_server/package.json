{
  "name": "auth_server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "tsx --test tests/*.test.ts",
    "test:watch": "tsx --test --watch tests/*.test.ts",
    "test:cov": "tsx --test tests/*.test.ts --coverage",
    "dev": "npx tsx watch --require tsconfig-paths/register server.ts",
    "build": "prisma generate --schema ../../shared/prisma/schema.prisma && tsc",
    "start": "node -r tsconfig-paths/register dist/apps/auth_server/server.js",
    "prisma:generate": "prisma generate --schema ../../shared/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema ../../shared/prisma/schema.prisma",
    "prisma:push": "prisma db push --schema ../../shared/prisma/schema.prisma",
    "prisma:studio": "cd ../../shared/prisma && prisma studio",
    "studio": "cd ../../shared/prisma && prisma studio --port 5556"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "5.22.0",
    "bcryptjs": "^3.0.3",
    "google-auth-library": "^10.5.0",
    "jsonwebtoken": "^9.0.3",
    "prom-client": "^15.1.3",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "prisma": "5.22.0",
    "tsx": "^4.20.6"
  }
}
